# Mantle L2 Deployment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
# NEVER commit your actual private key to version control
PRIVATE_KEY=your_private_key_here

# Mantle API key for contract verification (optional)
MANTLE_API_KEY=your_mantle_api_key_here

# Etherscan API key for contract verification on Ethereum (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas reporting (optional)
REPORT_GAS=false